{
  "name": "@cisco-sbg-ui/magna-react",
  "version": "1.0.0-beta.0",
  "description": "",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "scripts": {
    "build:babel-preset": "node build/generate-babel-preset.js",
    "build:lib": "babel --no-babelrc --config-file ./babel-lib.config.js framework --out-dir lib --copy-files",
    "build:docs": "next build",
    "build": "npm run build:lib && npm run build:babel-preset",
    "vercel-build": "npm run build:docs",
    "cover": "npm run cover:unit && npm run cover:integration && npm run coverage-report",
    "cover:unit": "nyc --silent npx cypress run-ct",
    "cover:integration": "nyc --silent --no-clean npm run test:components",
    "cover:merge": "npm run cover:unit && npm run cover:integration && nyc merge .nyc_output coverage.json",
    "coverage-report": "npx nyc report --reporter=text-summary",
    "cy:AthruC": "cypress run --env snapshots=off --spec \"framework/components/**/A[ABC]*\"",
    "cy:DthruL": "cypress run --env snapshots=off --spec \"framework/components/**/A[DEFGHIJKL]*\"",
    "cy:MthruS": "cypress run --env snapshots=off --spec \"framework/components/**/A[MNOPQRS]*\"",
    "cy:TthruZ": "cypress run --env snapshots=off --spec \"framework/components/**/A[TUVWXYZ]*\"",
    "cy:adhoc": "cypress run --env snapshots=$npm_config_snaponoff --spec \"framework/components/**/A[$npm_config_comprange]*\"",
    "cypress:install": "./node_modules/.bin/cypress install",
    "cypress:open": "cypress open --env snapshots=off",
    "cypress:run": "cypress run --env type=actual --browser chrome",
    "cypress:runUpdateSnapshots": "cypress run --env type=base --browser chrome",
    "esbuild:install": "node ./node_modules/esbuild/install.js",
    "husky:pre-commit": "lint-staged",
    "prepare": "husky install",
    "publish-package": "rimraf lib && npm ci && npm run build && npm publish",
    "dev": "next dev -p 8001",
    "start": "next start -p 8001",
    "start-ci": "next -p $npm_config_port || 3232",
    "test:dev": "cross-env WAIT_ON_TIMEOUT=600000 start-server-and-test dev http://localhost:8001 cypress:open",
    "test:lint": "eslint cypress docs framework pages --max-warnings 0",
    "test:components": "cross-env WAIT_ON_TIMEOUT=600000 INSTRUMENT=true start-server-and-test dev http://localhost:8001 cypress:run",
    "test": "npm run test:lint && npm run test:components",
    "test:all": "CYPRESS_baseUrl=http://localhost:3232 start-server-and-test start-ci :3232 cypress:run",
    "test:setOne": "CYPRESS_baseUrl=http://localhost:3233 start-server-and-test start-ci :3233 cy:AthruC",
    "test:setTwo": "CYPRESS_baseUrl=http://localhost:3234 start-server-and-test start-ci :3234 cy:DthruL",
    "test:setThree": "CYPRESS_baseUrl=http://localhost:3235 start-server-and-test start-ci :3235 cy:MthruS",
    "test:setFour": "CYPRESS_baseUrl=http://localhost:3236 start-server-and-test start-ci :3236 cy:TthruZ",
    "test:setAll": "npm run test:setOne & npm run test:setTwo & npm run test:setThree & npm run test:setFour",
    "test:setAdhoc": "CYPRESS_baseUrl=http://localhost:3237 start-server-and-test start-ci :3237 cy:adhoc",
    "updateSnapshots": "start-server-and-test dev http://localhost:3000 cypress:runUpdateSnapshots",
    "upload-coverage": "codecov",
    "formatIconFile": "prettier --config ./.prettierrc.js --write ./framework/components/AIcon/magnaIcons.js",
    "buildIcons": "node scripts/iconBuilder.mjs && npm run formatIconFile",
    "copyPhosphorIcons": "node scripts/copyPhosphorIcons.mjs",
    "copyAndBuildIcons": "npm run copyPhosphorIcons && npm run buildIcons"
  },
  "peerDependencies": {
    "prop-types": "^15.0.0",
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0"
  },
  "dependencies": {
    "babel-plugin-auto-import": "1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.18.10",
    "@babel/core": "7.19.1",
    "@babel/eslint-parser": "7.19.1",
    "@babel/preset-env": "7.19.1",
    "@babel/preset-react": "7.18.6",
    "@cypress/code-coverage": "3.9.12",
    "@cypress/react": "^5.12.5",
    "@cypress/webpack-dev-server": "^1.8.4",
    "babel-plugin-istanbul": "6.1.1",
    "codecov": "3.8.3",
    "cross-env": "7.0.3",
    "cypress": "9.7.0",
    "cypress-plugin-tab": "^1.0.5",
    "cypress-visual-regression": "1.7.0",
    "cypress-wait-until": "1.7.2",
    "dotenv": "16.0.2",
    "eslint": "8.23.1",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-anti-trojan-source": "1.1.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "fast-xml-parser": "^4.0.11",
    "glob": "8.0.3",
    "gray-matter": "4.0.3",
    "html-webpack-plugin": "^5.5.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "lodash-es": "^4.17.21",
    "lodash.debounce": "4.0.8",
    "next": "12.3.0",
    "next-mdx-remote": "4.2.0",
    "prettier": "2.7.1",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-docgen": "5.4.3",
    "react-dom": "17.0.2",
    "react-live": "3.1.1",
    "remark-slug": "6.1.0",
    "remove-markdown": "0.5.0",
    "rimraf": "3.0.2",
    "sass": "1.54.9",
    "start-server-and-test": "1.14.0",
    "webpack": "^5.72.1",
    "webpack-dev-server": "^4.11.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --max-warnings=0 --fix",
      "prettier --write"
    ],
    "*.json": "prettier --write",
    "*.html": "prettier --write",
    "*.scss": "prettier --write",
    "*.css": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cisco-sbg-ui/magna-react.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cisco-sbg-ui/magna-react/issues"
  },
  "homepage": "https://magna-react.security.cisco.com",
  "sideEffects": [
    "**/*.scss"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/cisco-sbg-ui"
  }
}
