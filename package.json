{
  "name": "@cisco-sbg-ui/magna-react",
  "version": "2.2.1",
  "description": "",
  "main": "lib/index.ts",
  "module": "lib/index.ts",
  "types": "lib/index.d.ts",
  "files": [
    "tokens",
    "framework",
    "lib",
    "babel-preset.js",
    "svg"
  ],
  "scripts": {
    "build:babel-preset": "node build/generate-babel-preset.js",
    "build:lib": "npm run build:types && babel --no-babelrc --config-file ./babel-lib.config.js framework --out-dir lib --copy-files",
    "build:docs": "next build",
    "build": "npm run clean && npm run addTokens && npm run build:types && npm run build:lib && npm run build:babel-preset",
    "vercel-build": "npm run addTokens && npm run build:docs",
    "cover": "npm run cover:unit && npm run cover:integration && npm run coverage-report",
    "cover:unit": "nyc --silent npx cypress run-ct",
    "cover:integration": "nyc --silent --no-clean npm run test:components",
    "cover:merge": "npm run cover:unit && npm run cover:integration && nyc merge .nyc_output coverage.json",
    "coverage-report": "npx nyc report --reporter=text-summary",
    "cypress:install": "./node_modules/.bin/cypress install",
    "esbuild:install": "node ./node_modules/esbuild/install.js",
    "husky:pre-commit": "lint-staged",
    "prepare": "husky install",
    "publish-package": "rimraf lib && npm ci && npm run build && npm publish",
    "dev": "npm run addTokens && next dev -p 8001 & npm run tsc",
    "start": "npm run addTokens next start -p 8001",
    "start-ci": "next -p $npm_config_port || 3232",
    "test:dev": "cypress open --component",
    "test:chrome": "cypress run --component --browser chrome",
    "test:firefox": "cypress run --component --browser firefox",
    "test:all": "npm run test:firefox && npm run test:chrome",
    "test:lint": "eslint cypress docs framework pages --max-warnings 0",
    "test": "npm run test:lint && npm run test:all",
    "updateSnapshots": "start-server-and-test dev http://localhost:3000 cypress:runUpdateSnapshots",
    "upload-coverage": "codecov",
    "formatIconFile": "prettier --config ./.prettierrc.js --write ./framework/components/AIcon/magnaIcons.js",
    "buildIcons": "node scripts/iconBuilder.mjs && npm run formatIconFile",
    "copyPhosphorIcons": "mkdir -p tmp && cp ./framework/components/AIcon/iconMap.js tmp/iconMap.mjs && node scripts/copyPhosphorIcons.mjs && rm -rf ./tmp",
    "copyAndBuildIcons": "npm run copyPhosphorIcons && npm run buildIcons",
    "modifyTokenClasses": "sh ./buildScripts/modifyCDSThemeClasses.sh",
    "addTokens": "rm -rf ./tokens && mkdir ./tokens && cp -r ./node_modules/@ciscodesignsystems/* ./tokens/ && npm run modifyTokenClasses",
    "tsc": "tsc -w",
    "build:types": "tsc -p tsconfig.json",
    "lint": "eslint . --ext .tsx --ext .jsx --ext .ts --ext .js",
    "clean": "rm -rf lib"
  },
  "peerDependencies": {
    "prop-types": "^15.0.0",
    "react": "^16.8.0 || ^17.0.2 || ^18.2.0",
    "react-dom": "^16.8.0 || ^17.0.2 || ^18.2.0"
  },
  "dependencies": {
    "babel-plugin-auto-import": "1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.24.7",
    "@babel/core": "7.24.7",
    "@babel/eslint-parser": "7.24.7",
    "@babel/plugin-transform-modules-commonjs": "^7.24.7",
    "@babel/preset-env": "7.24.7",
    "@babel/preset-react": "7.24.7",
    "@babel/preset-typescript": "7.24.7",
    "@ciscodesignsystems/cds-magnetic-design-tokens-web-css": "1.3.6",
    "@ciscodesignsystems/cds-magnetic-design-tokens-web-scss": "1.3.6",
    "@ciscodesignsystems/cds-magnetic-theme-web-css": "1.3.6",
    "@ciscodesignsystems/cds-magnetic-theme-web-scss": "1.3.6",
    "@cypress/code-coverage": "3.12.39",
    "@cypress/webpack-dev-server": "^3.4.1",
    "@tsconfig/recommended": "1.0.7",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "babel-plugin-istanbul": "6.1.1",
    "babel-plugin-typescript-to-proptypes": "2.1.0",
    "codecov": "3.8.3",
    "cross-env": "7.0.3",
    "cypress": "^13.13.0",
    "cypress-plugin-tab": "^1.0.5",
    "cypress-visual-regression": "1.7.0",
    "cypress-wait-until": "3.0.1",
    "dotenv": "16.0.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-anti-trojan-source": "1.1.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "fast-xml-parser": "^4.2.5",
    "glob": "8.0.3",
    "gray-matter": "4.0.3",
    "html-webpack-plugin": "^5.5.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "lodash.debounce": "4.0.8",
    "lodash.isequal": "^4.5.0",
    "next": "14.2.3",
    "next-mdx-remote": "4.4.1",
    "prettier": "2.7.1",
    "prism-react-renderer": "^1.3.5",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-docgen": "5.4.3",
    "react-docgen-typescript": "2.2.2",
    "react-dom": "18.2.0",
    "react-live": "3.1.1",
    "remark-gfm": "^3.0.1",
    "remark-slug": "6.1.0",
    "remove-markdown": "0.5.0",
    "rimraf": "3.0.2",
    "sass": "1.77.8",
    "start-server-and-test": "2.0.3",
    "typescript": "^5.5.2",
    "webpack": "^5.76.0",
    "webpack-dev-server": "^4.11.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --max-warnings=0 --fix",
      "prettier --write"
    ],
    "*.json": "prettier --write",
    "*.html": "prettier --write",
    "*.scss": "prettier --write",
    "*.css": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cisco-sbg-ui/magna-react.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cisco-sbg-ui/magna-react/issues"
  },
  "homepage": "https://magna-react.security.cisco.com",
  "sideEffects": [
    "**/*.scss"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/cisco-sbg-ui"
  }
}
